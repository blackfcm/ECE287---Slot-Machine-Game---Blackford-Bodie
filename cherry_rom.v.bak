module cherry_rom(
    input  wire        clk,
    input  wire [11:0] addr,
    output reg  [23:0] pixel   // 24-bit RGB
);
    reg [23:0] mem [0:1023];  // Example 32Ã—32 image

    initial begin
        $readmemh("cherry.mem", mem);
    end

    always @(posedge clk) begin
        pixel <= mem[addr];
    end

endmodule
